[push]
	default = simple

[credential]
        helper = store

[alias]
	pr = "!f() { git fetch -fu ${2:-origin} refs/pull/$1/head:pr/$1 && git checkout pr/$1; }; f"
	log5 = log --oneline --decorate --color -5
	logg = log --oneline --decorate --color --graph
